[alias]
a  = add
b  = branch
c  = commit
cm = commit -m
co = checkout
cb = checkout -b
d  = diff
ds = diff --staged
h  = log --graph --date=short --pretty=format:'%Cred%h%Creset %Cgreen%ad%Creset %s%d %Cblue%an%Creset'
p  = push
r  = rebase
ri = rebase -i
s  = status
sp = stash
sP = stash pop
u  = remote update
